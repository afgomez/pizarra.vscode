{
  "name": "Pizarra",
  "type": "dark",
  "colors": {
    "foreground": "#ebeced",

    // Base colors
    "editor.background": "#101216",
    "editor.foreground": "#ebeced",

    // Cursor
    "editorCursor.foreground": "#ffffff",
    "editorCursor.background": "#000000",

    // Cursor line
    "editor.lineHighlightBackground": "#00000059",
    "editor.lineHighlightBorder": "#0000",

    // Indent lines
    "editorIndentGuide.background": "#ebeced15",
    "editorIndentGuide.activeBackground": "#ebeced3d",

    // Selection
    "editor.selectionBackground": "#6a9bef75",
    "editor.inactiveSelectionBackground": "#6a9bef7F",
    "editor.selectionHighlightBackground": "#6a9bef3f",
    "editor.wordHighlightBackground": "#6a9bef3f",
    "editor.wordHighlightStrongBackground": "#6a9bef3f",

    // Search & replace
    "editor.findMatchBackground": "#DDC6775d",
    "editor.findMatchBorder": "#DDC677d0",

    "editor.findMatchHighlightBackground": "#DDC67731",
    "editor.findMatchHighlightBorder": "#DDC6775d",

    // Gutter
    "editorGutter.background": "#1C1F25FF",
    "editorLineNumber.foreground": "#3e424b",
    "editorLineNumber.activeForeground": "#ebeced",
    "editorGutter.modifiedBackground": "#52afe8",
    "editorGutter.deletedBackground": "#D64933",
    "editorGutter.addedBackground": "#97D633",

    // ActivityBar
    "activityBar.background": "#101216",
    "activityBarBadge.background": "#4089b5",

    // Sidebar
    "sideBarTitle.foreground": "#d8d9db",
    "sideBar.background": "#08090b",

    // Tabline
    "tab.activeForeground": "#ebeced",
    "tab.inactiveBackground": "#08090b",
    "tab.inactiveForeground": "#64686f",

    // Squiggles
    "editorError.foreground": "#D64933",
    "editorWarning.foreground": "#D6B233",

    // Terminal
    "terminal.background": "#08090b",
    "terminal.foreground": "#ebeced",
    "terminalCursor.foreground": "#ffffff",

    "terminal.ansiBlack": "#040506",
    "terminal.ansiBrightBlack": "#64686f",
    "terminal.ansiRed": "#D64933",
    "terminal.ansiBrightRed": "#DD8577",
    "terminal.ansiGreen": "#97D633",
    "terminal.ansiBrightGreen": "#B5DD77",
    "terminal.ansiYellow": "#D6B233",
    "terminal.ansiBrightYellow": "#DDC677",
    "terminal.ansiBlue": "#4089b5",
    "terminal.ansiBrightBlue": "#52afe8",
    "terminal.ansiMagenta": "#D65191",
    "terminal.ansiBrightMagenta": "#DD96B8",
    "terminal.ansiCyan": "#67EDFF",
    "terminal.ansiBrightCyan": "#B4F6FF",
    "terminal.ansiWhite": "#ebeced",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#eeffffff",
        "background": "#263238ff"
      }
    },

    // Comments
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#64686f"
      }
    },

    // Strings and chars
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#97D633"
      }
    },

    {
      "name": "Escape sequence",
      "scope": ["string constant.character.escape"],
      "settings": {
        "foreground": "#B5DD77",
        "fontStyle": "italic"
      }
    },

    // String interpolation
    {
      "name": "Interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#B5DD77"
      }
    },

    {
      "name": "Embedded",
      "scope": ["meta.template.expression", "source meta.embedded"],
      "settings": {
        "foreground": "#ebeced"
      }
    },

    // Regexp
    {
      "name": "String",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#9AFFCC"
      }
    },

    // Constants:
    {
      "name": "Constant",
      "scope": [
        // numbers, booleans, ruby symbols, etc.
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.symbol"
      ],
      "settings": {
        "foreground": "#D65191"
      }
    },

    // `this` / `self`
    {
      "name": "this",
      "scope": [
        // Javascript
        "variable.language.this",

        // Ruby
        "variable.language.self",

        // Python
        "variable.language.special.self",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "foreground": "#DD96B8"
      }
    },

    // Keywords
    {
      "name": "Keyword",
      "scope": [
        // Generic
        "keyword",

        // Javascript: const, let, var, class, function keywords
        "storage.type",

        // JS/TS extends, implements, public
        "storage.modifier",

        // Python type annotations
        "punctuation.separator.annotation"
      ],
      "settings": {
        "foreground": "#D67133"
      }
    },

    // Identifiers: function, class and method names
    {
      "name": "Identifier",
      "scope": [
        // JS
        "meta.definition entity.name",
        "meta.class entity.name.type",
        "meta.method.declaration storage.type", // Constructor

        // TS
        "meta.enum.declaration entity.name.type",
        "meta.interface entity.name.type",

        // Ruby
        "meta.class.ruby entity.name",
        "meta.module.ruby entity.name",
        "meta.function.method entity.name",

        // Python
        "meta.class.python entity.name",
        "meta.function.python entity.name",

        // SCSS
        "meta.at-rule.function support.function",
        "meta.at-rule.mixin entity.name",

        // SASS
        "support.function.name entity.name"
      ],
      "settings": {
        "foreground": "#D6B233"
      }
    },

    // Types
    {
      "name": "Type",
      "scope": [
        // Var and parameter annotations
        "meta.type.annotation entity.name.type",
        "meta.type.annotation support.type",
        "meta.type.annotation meta.brace",

        // Generics
        "meta.type.parameters entity.name.type",
        "meta.type.parameters punctuation.definition",

        // Return types
        "meta.return.type entity.name.type",

        // Type casts
        "cast.expr",

        // Python
        "support.type.python",
        "comment.typehint.type",
        "comment.typehint.punctuation"
      ],
      "settings": {
        "foreground": "#DDC677"
      }
    },

    // Inheritance
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class",
        "meta.class entity.name.type.module"
      ],
      "settings": {
        "foreground": "#67EDFF",
        "fontStyle": "italic"
      }
    },

    // Decorators
    {
      "name": "Decorator",
      "scope": [
        "meta.function.decorator entity.name.function",
        "meta.function.decorator punctuation.definition",
        "meta.decorator entity.name.function",
        "meta.decorator punctuation.decorator",
        "meta.decorator meta.brace"
      ],
      "settings": {
        "foreground": "#94c5cc"
      }
    },

    // XML/HTML/JSX
    {
      "name": "XML Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#67EDFF"
      }
    },

    {
      "name": "HTML Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attributes keyword.operator.assignment",
        "meta.tag.attributes punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#b4f6ff"
      }
    },

    // CSS/SCSS/SASS
    {
      "name": "CSS property name",
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#b4f6ff"
      }
    },

    {
      "name": "CSS vendor property name",
      "scope": ["support.type.vendored.property-name"],
      "settings": {
        "foreground": "#b4f6ff",
        "fontStyle": "italic"
      }
    },

    {
      "name": "CSS property value",
      "scope": [
        // Normal properties
        "support.constant.property-value",

        // Colors
        "support.constant.color",
        "constant.other.color",

        // Fonts
        "support.constant.font-name",

        // Funtions
        "meta.property-value support.function",

        // SCSS include/extend
        "meta.at-rule.include entity.name.function",
        "meta.at-rule.extend entity.other.attribute-name.placeholder"
      ],
      "settings": {
        "foreground": "#DDC677"
      }
    },

    {
      "name": "CSS Selector: #id",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#DD8577"
      }
    },

    {
      "name": "CSS Selector: .class",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#4DFFA5"
      }
    },

    {
      "name": "CSS Selector: [attribute]",
      "scope": ["meta.attribute-selector", "meta.attribute-selector keyword"],
      "settings": {
        "foreground": "#b4f6ff"
      }
    },

    {
      "name": "CSS Important",
      "scope": ["keyword.other.important"],
      "settings": {
        "foreground": "#D64933"
      }
    },

    // SCSS: Extend declaration
    {
      "name": "SCSS Extend",
      "scope": ["entity.other.attribute-name.placeholder"],
      "settings": {
        "foreground": "#D6B233"
      }
    },

    // SCSS: reference
    {
      "name": "SCSS Reference",
      "scope": ["meta.property-list entity.name.tag.reference"],
      "settings": {
        "foreground": "#DD96B8"
      }
    },

    // Markdown
    {
      "name": "Markup heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Markup bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Markup italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Markup monospace",
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": {
        "foreground": "#ffffff"
      }
    },

    {
      "name": "Markup fenced code",
      "scope": ["markup.fenced_code punctuation.definition"],
      "settings": {
        "foreground": "#ffffff"
      }
    },

    // JSDoc
    {
      "name": "JSDoc: keyword",
      "scope": ["comment storage.type"],
      "settings": {
        "foreground": "#DD8577"
      }
    },
    {
      "name": "JSDoc: type",
      "scope": ["comment entity.name.type"],
      "settings": {
        "foreground": "#DDC677"
      }
    },
    {
      "name": "JSDoc: variable",
      "scope": ["comment variable.other"],
      "settings": {
        "foreground": "#ebeced"
      }
    },

    // TODO/FIXME - Depends on vscode-language-todo
    {
      "name": "TODO",
      "scope": [
        // python
        "comment keyword.codetag.notation",

        // vscode-language-todo
        "comment storage.type.class.todo",
        "comment storage.type.class.fixme",
        "comment storage.type.class.changed",
        "comment storage.type.class.xxx",
        "comment storage.type.class.idea",
        "comment storage.type.class.hack",
        "comment storage.type.class.note",
        "comment storage.type.class.review",
        "comment storage.type.class.nb",
        "comment storage.type.class.bug",
        "comment storage.type.class.question",
        "comment storage.type.class.combak",
        "comment storage.type.class.temp",
        "comment storage.type.class.debug",
        "comment storage.type.class.optimize"
      ],
      "settings": {
        "foreground": "#4089b5",
        "fontStyle": "italic"
      }
    }
  ]
}
